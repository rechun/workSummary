var numbers = [5,6,2,3,7];

var max = Math.max.apply(null,numbers);

console.log(max);


call()方法接受的是参数列表，


function Product(name,price){
  this.name = name;
  this.price = price;
}
function Food (name,price){
        Product.call(this,name,price);
        this.category = 'food';
}
console.log(new Food('cheese',5).name);//cheese
